{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "allowJs": true,
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "importHelpers": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
