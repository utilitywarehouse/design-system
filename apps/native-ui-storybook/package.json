{
  "name": "native-ui-storybook",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules &&  rm -rf .expo",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:preview:android": "EAS_LOCAL_BUILD_WORKINGDIR='./eas-build' eas build  --profile preview -p android --local --output './build/native-ui.apk'",
    "build:preview:ios": "EAS_LOCAL_BUILD_WORKINGDIR='./eas-build' eas build  --profile preview -p ios --local --output './build/native-ui.tar.gz'",
    "eas-build-pre-install": "echo '//registry.npmjs.org/:_authToken=${NPM_TOKEN}' > temp.txt && echo 'registry=https://registry.npmjs.org/' >> temp.txt && cat ../../.npmrc >> temp.txt && mv temp.txt ../../.npmrc",
    "eas-build-post-install": "if [[ \"$OSTYPE\" == \"darwin\"* ]]; then sed -i '.bak' '/\\/\\/registry.npmjs.org\\/:_authToken=${NPM_TOKEN}/d' ../../.npmrc && sed -i '.bak' '/registry=https:\\/\\/registry.npmjs.org\\//d' ../../.npmrc && rm ../../.npmrc.bak; else sed -i '/\\/\\/registry.npmjs.org\\/:_authToken=${NPM_TOKEN}/d' ../../.npmrc && sed -i '/registry=https:\\/\\/registry.npmjs.org\\//d' ../../.npmrc; fi",
    "storybook:web": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook": "cross-env STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env STORYBOOK_ENABLED='true' expo start --android"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/slider": "4.5.2",
    "@storybook/native": "^3.1.2",
    "@storybook/native-addon": "^3.1.2",
    "@storybook/native-components": "^3.1.2",
    "@storybook/react-native-theming": "^7.6.18",
    "@utilitywarehouse/colour-system": "workspace:^",
    "@utilitywarehouse/fontsource": "^0.0.6",
    "@utilitywarehouse/native-ui": "workspace:^",
    "@utilitywarehouse/react-icons": "^1.1.1",
    "@utilitywarehouse/react-native-icons": "^1.5.0",
    "expo": "~51.0.6",
    "expo-constants": "~16.0.1",
    "expo-dev-client": "~4.0.13",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-markdown-display": "^7.0.2",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-svg": "^15.2.0",
    "react-native-web": "~0.19.11",
    "storybook-dark-mode": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@expo/metro-runtime": "^3.2.1",
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-controls": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-mdx-gfm": "^7.6.17",
    "@storybook/addon-ondevice-actions": "^7.6.18",
    "@storybook/addon-ondevice-backgrounds": "^7.6.18",
    "@storybook/addon-ondevice-controls": "^7.6.18",
    "@storybook/addon-ondevice-notes": "^7.6.18",
    "@storybook/addon-react-native-web": "^0.0.23",
    "@storybook/react": "^7.6.17",
    "@storybook/react-native": "^7.6.18",
    "@storybook/react-webpack5": "^7.6.17",
    "@types/react": "~18.2.79",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.11",
    "cross-env": "^7.0.3",
    "entities": "2.0.3",
    "patch-package": "^8.0.0",
    "storybook": "^7.6.17",
    "tocbot": "^4.27.20",
    "typescript": "^5.3.3",
    "unzipper": "^0.11.6"
  },
  "overrides": {
    "react-docgen-typescript": "2.2.2"
  },
  "private": true
}
