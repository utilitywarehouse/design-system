    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-mdx-gfm": "^7.6.17",
    "@storybook/addon-ondevice-actions": "^7.6.14",
    "@storybook/addon-ondevice-backgrounds": "^7.6.14",
    "@storybook/addon-ondevice-controls": "^7.6.14",
    "@storybook/addon-ondevice-notes": "^7.6.14",
    "@storybook/addon-react-native-web": "^0.0.22",
    "@storybook/react": "^7.6.17",
    "@storybook/react-native": "^7.6.14",
    "@storybook/react-webpack5": "^7.6.17",
    "@types/react": "~18.2.45",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.10",
    "cross-env": "^7.0.3",
    "patch-package": "^8.0.0",
