{
  "name": "native-ui-storybook",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules &&  rm -rf .expo",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:preview:android": "eas build  --profile preview -p android --local --output './build/native-ui.apk'",
    "build:preview:ios": "eas build  --profile preview -p ios --local --output './build/native-ui.tar.gz'",
    "storybook:web": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook": "cross-env STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env STORYBOOK_ENABLED='true' expo start --android"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-native-community/slider": "4.4.2",
    "@storybook/native": "^3.1.0",
    "@storybook/native-addon": "^3.1.0",
    "@storybook/native-components": "^3.1.0",
    "@storybook/react-native-theming": "^7.6.15",
    "@utilitywarehouse/colour-system": "workspace:^",
    "@utilitywarehouse/fontsource": "^0.0.6",
    "@utilitywarehouse/native-ui": "workspace:^",
    "@utilitywarehouse/react-icons": "^1.1.1",
    "@utilitywarehouse/react-native-icons": "^1.2.0",
    "expo": "~50.0.11",
    "expo-constants": "~15.4.5",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-markdown-display": "^7.0.2",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-svg": "^14.1.0",
    "react-native-web": "~0.19.10",
    "storybook-dark-mode": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@expo/metro-runtime": "^3.1.3",
    "@storybook/addon-actions": "^7.6.17",
