{
  "name": "@utilitywarehouse/web-ui",
  "version": "0.0.1",
  "description": "React components for building UW UIs",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "build": "rm -rf dist/ && tsc --build ./tsconfig.json",
    "lint:fix": "TIMING=1 eslint --fix --ext .ts,.tsx,.js --ignore-pattern *.mdx \"./src/**\"",
    "lint": "TIMING=1 eslint --ext .ts,.tsx,.js --ignore-pattern *.mdx \"./src/**\"",
    "precommit": "lint-staged",
    "dev": "tsc --watch",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook --quiet -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "pnpm lint"
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@utilitywarehouse/customer-ui-design-tokens": "^2.0.2",
    "@utilitywarehouse/customer-ui-react-icons": "^1.0.2"
  },
  "devDependencies": {
    "@mui/material": "^5.10.5",
    "@mui/system": "^5.10.5",
    "@mui/types": "^7.1.4",
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "tsup": "^6.2.3",
    "@storybook/react": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@babel/core": "^7.20.12",
    "babel-loader": "^8.3.0"
  },
  "peerDependencies": {
    "@mui/material": "^5.10.5",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "files": [
    "dist"
  ]
}
