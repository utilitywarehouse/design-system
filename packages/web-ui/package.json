{
  "name": "@utilitywarehouse/web-ui",
  "version": "1.0.4",
  "description": "React components for building UW UIs",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "build": "NODE_OPTIONS='--max-old-space-size=16384' tsup",
    "ts-types": "tsc",
    "lint:fix": "TIMING=1 eslint --fix --max-warnings 0 --ext ts,tsx src",
    "lint": "TIMING=1 eslint --max-warnings 0 --ext ts,tsx src",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "format:ci": "prettier --list-different \"./{src,types}/**/*.{tsx,ts}\"",
    "dev": "storybook dev --quiet -p 6006",
    "build:storybook": "storybook build --quiet",
    "build-storybook": "storybook build",
    "chromatic": "chromatic",
    "figma:publish": "figma connect publish",
    "cc": "node ./scripts/create-components.cjs"
  },
  "author": "",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/is-prop-valid": "^1.2.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/system": "6.1.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-use-controllable-state": "^1.0.1",
    "@utilitywarehouse/colour-system": "workspace:^",
    "@utilitywarehouse/react-icons": "^1.1.1",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@mui/material": "5.14.15",
    "@mui/types": "7.2.17",
    "@storybook/addon-a11y": "^7.6.20",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/blocks": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "@storybook/react-webpack5": "^7.6.20",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^18.15.11",
    "@types/react": "^17.0.0 || ^18.0.0",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@utilitywarehouse/css-reset": "workspace:^",
    "@utilitywarehouse/fontsource": "^0.1.0",
    "@vercel/analytics": "^1.3.1",
    "chalk": "4",
    "chromatic": "^11.3.5",
    "commander": "10",
    "prop-types": "^15.8.1",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.0",
    "storybook": "^7.6.20",
    "tsup": "^8.3.0"
  },
  "peerDependencies": {
    "@mui/material": "^5.16.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "files": [
    "dist"
  ]
}
