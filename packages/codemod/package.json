{
  "name": "@utilitywarehouse/ds-codemod",
  "version": "0.0.6",
  "description": "Codemod scripts for Design Systems packages.",
  "bin": {
    "@utilitywarehouse/ds-codemod": "bin/codemod.js"
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "format": "prettier --write \"./{transforms,bin}/**/*.{ts,tsx,js}\"",
    "format:ci": "prettier --list-different \"./{transforms,bin}/**/*.{tsx,ts,js}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utilitywarehouse/design-systems.git",
    "directory": "packages/codemod"
  },
  "author": "Rob Phoenix",
  "license": "UNLICENSED",
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "1.0.0-rc.12",
    "execa": "^8.0.1",
    "globby": "^14.0.1",
    "inquirer": "^9.2.19",
    "is-git-clean": "^1.1.0",
    "jscodeshift": "^0.15.2",
    "meow": "^13.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@utilitywarehouse/web-ui": "workspace:^",
    "babel-jest": "^29.7.0",
    "jest": "^29.3.1"
  },
  "files": [
    "bin",
    "**/*.js"
  ],
  "jest": {
    "transform": {
      "\\*.[jt]sx?$": "babel-jest"
    }
  }
}
