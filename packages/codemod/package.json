{
  "name": "@utilitywarehouse/web-ui-codemod",
  "version": "0.0.1",
  "description": "Codemode scripts for Web UI",
  "bin": {
    "@utilitywarehouse/web-ui-codemod": "dist/bin/codemod.js"
  },
  "scripts": {
    "build": "rm -rf dist && pnpm tsc -d -p tsconfig.json",
    "dev": "pnpm tsc -d -w -p tsconfig.json",
    "test": "jest",
    "format": "prettier --write \"./{transforms,bin}/**/*.{ts,tsx,js}\"",
    "format:ci": "prettier --list-different \"./{transforms,bin}/**/*.{tsx,ts,js}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utilitywarehouse/design-systems.git",
    "directory": "packages/codemod"
  },
  "author": "Rob Phoenix",
  "license": "UNLICENSED",
  "dependencies": {
    "@utilitywarehouse/web-ui": "workspace:^",
    "chalk": "4.1.0",
    "cheerio": "1.0.0-rc.9",
    "execa": "4.0.3",
    "globby": "11.0.1",
    "inquirer": "7.3.3",
    "is-git-clean": "1.1.0",
    "jscodeshift": "0.13.1",
    "meow": "7.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/jscodeshift": "0.11.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3"
  },
  "files": [
    "dist"
  ]
}
