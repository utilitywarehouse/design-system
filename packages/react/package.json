{
  "name": "@utilitywarehouse/uw-web-ui",
  "version": "0.0.1",
  "description": "UW Web UI components",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "build": "rm -rf dist/ && tsc --build ./tsconfig.json",
    "lint:fix": "TIMING=1 eslint --fix --ext .ts,.tsx,.js --ignore-pattern *.mdx \"./src/**\"",
    "lint": "TIMING=1 eslint --ext .ts,.tsx,.js --ignore-pattern *.mdx \"./src/**\"",
    "precommit": "lint-staged",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "pnpm lint"
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@mui/utils": "^5.10.9",
    "@utilitywarehouse/customer-ui-design-tokens": "^1.0.3",
    "@utilitywarehouse/customer-ui-react-icons": "^1.0.2",
    "@utilitywarehouse/uw-ui-fonts": "^0.0.3",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.5",
    "@mui/system": "^5.10.5",
    "@mui/types": "^7.1.4",
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "tsup": "^6.2.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.5",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "files": [
    "dist"
  ]
}
