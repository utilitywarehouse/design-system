{
  "name": "@utilitywarehouse/customer-ui-theme",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "yarn run clean && tsc",
    "watch": "yarn run build -- --watch",
    "format": "prettier --write \"./src/**/*.{ts,js}\"",
    "lint": "eslint ./src --fix --ext .ts,.js",
    "format:ci": "prettier --list-different \"./src/**/*.{ts,js}\"",
    "lint:ci": "eslint ./src --ext .ts,.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/*.ts": [
      "yarn lint"
    ]
  },
  "license": "ISC",
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@utilitywarehouse/customer-ui-design-tokens": "1.0.0-alpha.10",
    "@ctrl/tinycolor": "^3.4.0"
  }
}
