{
  "name": "@utilitywarehouse/customer-ui-material",
  "version": "0.1.0",
  "description": "UW Customer UI component library, built upon MUI Material",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "yarn run clean && tsc",
    "test:coverage": "yarn test --coverage",
    "serve:coverage": "open coverage/lcov-report/index.html",
    "lint": "eslint --fix --ext .ts,.tsx,.js \"./src/**\" \"./stories/**\"",
    "lint:ci": "eslint --ext .ts,.tsx,.js \"./src/**\" \"./stories/**\"",
    "format": "prettier --write \"./{src,stories}/**/*.{ts,tsx,js}\"",
    "format:ci": "prettier --list-different \"./{src,stories}/**/*.{ts,tsx,js}\"",
    "precommit": "lint-staged",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o storybook --loglevel warn"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.3.2",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.4",
    "@mui/styles": "^5.0.1",
    "@utilitywarehouse/customer-ui-theme": "1.0.0-alpha.21"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-controls": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "@types/enzyme": "^3.10.8",
    "@types/react-dom": "^17.0.0",
    "@utilitywarehouse/customer-ui-fonts": "^1.0.0-alpha.4",
    "@utilitywarehouse/customer-ui-react-icons": "^1.0.0-alpha.13",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "storybook-dark-mode": "^1.0.8",
    "style-loader": "^2.0.0"
  }
}
