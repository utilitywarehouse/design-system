{
  "name": "@utilitywarehouse/customer-ui-material",
  "version": "1.0.8",
  "description": "UW Customer UI component library, built upon MUI Material",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf node_modules",
    "build": "rimraf dist/ && tsc --build ./tsconfig.build.json",
    "test:coverage": "pnpm test --coverage",
    "serve:coverage": "open coverage/lcov-report/index.html",
    "lint": "eslint --fix --ext .ts,.tsx,.js \"./src/**\" \"./stories/**\"",
    "lint:ci": "eslint --ext .ts,.tsx,.js \"./src/**\" \"./stories/**\"",
    "format": "prettier --write \"./{src,stories}/**/*.{ts,tsx,js}\"",
    "format:ci": "prettier --list-different \"./{src,stories}/**/*.{ts,tsx,js}\"",
    "precommit": "lint-staged",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o storybook-build",
    "storybook:ci": "npm install && build-storybook -o storybook-build"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "pnpm format",
      "pnpm lint"
    ]
  },
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.0",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.7",
    "@mui/styled-engine": "^5.8.7",
    "@mui/styles": "^5.8.7",
    "@utilitywarehouse/customer-ui-design-tokens": "^1.0.3",
    "@utilitywarehouse/customer-ui-fonts": "^1.0.1",
    "@utilitywarehouse/customer-ui-react-icons": "^1.0.2",
    "jss": "^10.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@mui/types": "^7.1.4",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-viewport": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/theming": "^6.5.9",
    "@types/react": "^17.0.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0"
  },
  "files": [
    "dist"
  ]
}
