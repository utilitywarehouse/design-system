{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", "storybook-static/**"],
      "dependsOn": ["^build"]
    },
    "checks": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build", "^lint"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "build:storybook": {
      "outputs": ["apps/storybook/storybook-static/**"]
    }
  }
}
